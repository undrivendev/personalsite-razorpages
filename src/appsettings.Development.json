{
  "DetailedErrors": true,
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "Enrich": ["FromLogContext"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  }
}
